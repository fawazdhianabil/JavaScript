function perkalian(a,b){
	return a*b
}
perkalian(2,3);

function perkurangan(a, b) {
    return a-b;
}

function pembagian(a,b) {
	return a/b
}

function penjumlahan(a,b) {
	return a+b
}

console.log("Hasil Perkalian :" + perkalian(3, 4));
console.log("Hasil Pembagian :" + pembagian(3, 4));
console.log("Hasil Pengurangan :" + perkurangan(3, 4));
console.log("Hasil Penjumlahan :" + penjumlahan(3, 4));